{
  "name": "ngrx-lite",
  "version": "15.0.0",
  "license": "MIT",
  "scripts": {
    "affected:build": "nx affected --target=build",
    "affected:lint": "nx affected:lint",
    "affected:test": "nx affected:test",
    "affected:e2e": "nx affected --target=e2e",
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2020 browser module main",
    "start": "nx run sample-app:serve",
    "build": "nx build",
    "test": "nx test",
    "build:release": "nx run store:build:production",
    "build:docs": "nx run doc-app:build && nx run sample-app:build:production --baseHref=/ngrx-lite/sample-app/ --outputPath=./dist/apps/doc-app/sample-app",
    "release": "release-it",
    "release:pre": "release-it --preRelease",
    "release:patch": "release-it --increment patch",
    "release:minor": "release-it --increment minor",
    "release:major-pre": "release-it --preRelease --increment major",
    "release:major": "release-it --increment major",
    "release:dry": "release-it --release-version"
  },
  "release-it": {
    "plugins": {
      "@release-it-plugins/workspaces": {
        "workspaces": [
          "libs/store"
        ]
      }
    },
    "npm": false
  },
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/gernsdorfer/ngrx-lite.git"
  },
  "dependencies": {
    "@angular/animations": "15.0.1",
    "@angular/cdk": "15.0.0",
    "@angular/common": "15.0.1",
    "@angular/compiler": "15.0.1",
    "@angular/core": "15.0.1",
    "@angular/forms": "15.0.1",
    "@angular/material": "15.0.0",
    "@angular/platform-browser": "15.0.1",
    "@angular/platform-browser-dynamic": "15.0.1",
    "@angular/router": "15.0.1",
    "@ngrx/component-store": "14.3.2",
    "@ngrx/effects": "15.0.0-rc.0",
    "@ngrx/entity": "15.0.0-rc.0",
    "@ngrx/store": "15.0.0-rc.0",
    "@ngrx/store-devtools": "15.0.0-rc.0",
    "@nrwl/angular": "15.2.1",
    "rxjs": "~7.5.5",
    "tslib": "^2.4.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "15.0.1",
    "@angular-eslint/eslint-plugin": "15.1.0",
    "@angular-eslint/eslint-plugin-template": "15.1.0",
    "@angular-eslint/template-parser": "15.1.0",
    "@angular/cli": "~15.0.0",
    "@angular/compiler-cli": "15.0.1",
    "@angular/language-service": "15.0.1",
    "@docusaurus/core": "2.0.0-beta.13",
    "@docusaurus/module-type-aliases": "2.0.0-beta.13",
    "@docusaurus/preset-classic": "2.0.0-beta.13",
    "@mdx-js/react": "^1.6.21",
    "@nrwl/cli": "15.2.1",
    "@nrwl/cypress": "15.2.1",
    "@nrwl/eslint-plugin-nx": "15.2.1",
    "@nrwl/linter": "15.2.1",
    "@nrwl/nx-cloud": "15.0.2",
    "@nrwl/workspace": "15.2.1",
    "@nx-plus/docusaurus": "14.1.0",
    "@release-it-plugins/workspaces": "^3.2.0",
    "@types/jasmine": "4.3.0",
    "@types/node": "17.0.41",
    "@typescript-eslint/eslint-plugin": "5.44.0",
    "@typescript-eslint/parser": "5.44.0",
    "clsx": "^1.1.1",
    "cypress": "^11.0.0",
    "eslint": "8.15.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-cypress": "^2.10.3",
    "jasmine-core": "4.5.0",
    "jasmine-marbles": "^0.9.2",
    "jasmine-spec-reporter": "7.0.0",
    "karma": "6.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.0.0",
    "ng-packagr": "15.0.1",
    "nx": "15.2.1",
    "postcss": "^8.4.19",
    "postcss-import": "14.1.0",
    "postcss-preset-env": "~7.7.1",
    "postcss-url": "10.1.3",
    "prettier": "2.8.0",
    "prettier-plugin-organize-imports": "^3.2.1",
    "prism-react-renderer": "^1.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "release-it": "^15.5.0",
    "release-it-lerna-changelog": "^5.0.0",
    "release-it-yarn-workspaces": "^3.0.0",
    "ts-jest": "28.0.1",
    "typescript": "4.8.4"
  }
}
